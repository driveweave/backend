{
  "name": "driveweave",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www --legacy-watch"
  },
  "nodemonConfig": {
    "watch": [
      "/usr/driveweaveserver"
    ],
    "restartable": "rs",
    "ignore": [
      "node_modules/",
      "build/"
    ],
    "legacyWatch": true,
    "delay": "0",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,www"
  },
  "engines": {
    "node": "20.9.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "check-password-strength": "^2.0.7",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "~1.9.1",
    "node-url-shortener": "^2.0.3",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
